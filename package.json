{
  "name": "css-classes-from-html",
  "displayName": "CSS Classes from HTML - Figma Integration",
  "description": "Автоматична генерація CSS з HTML та інтеграція з Figma",
  "version": "0.0.7",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "dev": "node frontend/server.js",
    "test": "node test/run-tests.js",
    "build": "node build.js",
    "validate": "node utils/ValidationUtils.js",
    "optimize": "node utils/OptimizationUtils.js",
    "config": "node frontend/config-ui.js"
  },
  "keywords": [
    "css",
    "html",
    "figma",
    "vscode",
    "style-generation",
    "automation",
    "design-to-code"
  ],
  "activationEvents": [
    "onCommand:cssclasssfromhtml.generateCSS",
    "onLanguage:html"
  ],
  "keybindings": [
    {
      "key": "ctrl+shift+h",
      "command": "cssclasssfromhtml.generateCSS",
      "when": "editorTextFocus"
    },
    {
      "key": "ctrl+shift+r",
      "command": "cssclasssfromhtml.repeatLastAction",
      "when": "editorTextFocus"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VuToV-Mykola/css-classes-from-html.git"
  },
  "bugs": {
    "url": "https://github.com/VuToV-Mykola/css-classes-from-html/issues"
  },
  "homepage": "https://github.com/VuToV-Mykola/css-classes-from-html#readme",
  "author": "VuToV-Mykola",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "figma-api": "^2.0.2-beta",
    "node-fetch": "^3.3.0",
    "open": "^8.4.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8"
  }
}
